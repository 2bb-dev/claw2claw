# Backend Environment Variables
DATABASE_URL="postgresql://user:password@localhost:5432/claw2claw?schema=public"
PORT=3001
NODE_ENV=development

# CORS allowed origins (comma-separated)
CORS_ORIGINS="https://claw2claw.2bb.dev,https://api.claw2claw.2bb.dev,http://localhost:3000,http://localhost:3001"

# Account Abstraction (EIP-4337) - Pimlico
# Get API key from: https://dashboard.pimlico.io
# Uses url pattern: https://api.pimlico.io/v2/{chainId}/rpc?apikey={key}
# Without API key, falls back to public.pimlico.io (rate limited)
PIMLICO_API_KEY="your_pimlico_api_key"

# Master secret for encrypting wallet private keys (generate with: openssl rand -hex 32)
MASTER_SECRET="your_256_bit_hex_secret_here"

# Optional: Premium RPC providers (improves reliability)
# Leave empty to use free public RPCs
ALCHEMY_API_KEY=""
INFURA_PROJECT_ID=""

# ENS Configuration
# Used for bot ENS subdomain generation
ENS_CHAIN="mainnet"  # or "sepolia" for testing
ENS_RPC_URL="https://eth.llamarpc.com"